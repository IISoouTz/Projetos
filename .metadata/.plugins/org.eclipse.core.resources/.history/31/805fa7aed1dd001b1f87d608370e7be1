package entities;

public class carroSemiNovo extends carroNovo {

	
	public float valorAdcional;
	
	public carroSemiNovo() {
	
	}
	
	public carroSemiNovo(String montadora, int anoFabricacao, float preco, float valorAdicional) {
		super(montadora, anoFabricacao, preco);
		this.valorAdicional=valorAdicional;
		this.valorAdicional=valorAdicional;
	}
}
