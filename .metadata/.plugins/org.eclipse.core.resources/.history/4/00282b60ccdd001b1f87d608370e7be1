package entities;

public class Funcionario extends PessoaFisica {

	private String cartao;

	public Funcionario() {
		
	}
	
	public Funcionario (String nome, String rg, String cartao) {
		
	}
	
	public String getCartao() {
		return cartao;
	}

	public void setCartao(String cartao) {
		this.cartao = cartao;
	}
	
}
